type (
	addReq {
		article string `form:"article"`
		
	}
	
	addResp {
		ok bool `json:"ok"`
	}
)

type (
	checkReq {
		article string `form:"article"`
	}
	
	checkResp {
		found bool `json:"found"`
	}
)

service bookstore-api {
	@handler AddHandler
	get /add (addReq) returns (addResp)
	
	@handler CheckHandler
	get /check (checkReq) returns (checkResp)
}